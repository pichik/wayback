	// FB script
	function initFB()
	{
	  var fbInit = setTimeout(function () { initFBJS(document, 'script', 'facebook-jssdk'); }, 3000);
	}

	function initFBJS(d, s, id)
	{
	  var js, fjs = d.getElementsByTagName(s)[0];
	  if (d.getElementById(id)) return;
	  js = d.createElement(s); js.id = id;
	  js.async = true; js.src = _fbConnectUrl;
	  fjs.parentNode.insertBefore(js, fjs);

	  window.fbAsyncInit = function ()
	  {
		FB.init({
		  appId: _fbConnectId,
		  status: true,
		  xfbml: true,
		  cookie: true
		});
	  };
	}

    initFB();

	</script>

